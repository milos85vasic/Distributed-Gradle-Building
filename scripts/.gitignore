# Scripts specific .gitignore for distributed build system

# Executable files (keep .sh files, ignore binaries)
*.exe
!*.sh

# Temporary files
*.tmp
*.temp
tmp/
temp/

# Log files
*.log
logs/

# Lock files
*.lock
*.pid

# Data and state files
data/
state/
pids.txt
*.state

# Configuration overrides
*.local
*.override
config.local.properties
gradle.properties.local

# Cache directories
cache/
gradle-cache/
build-cache/

# Performance and metrics files
perf-results/
metrics/
monitoring/
test-results/
coverage/

# Backup files
*.bak
*.backup
*.old

# Editor files
*~
*.swp
*.swo
.vimrc
.netrwhist

# OS files
.DS_Store
.DS_Store?
._*
Thumbs.db
ehthumbs.db

# Sensitive data
secrets/
.passwords
*.key
*.pem
*.p12
id_rsa*
known_hosts

# Environment files
.env
.env.local
.env.*

# Test output
test-output/
reports/

# Generated files
generated/

# Compressed archives
*.tar.gz
*.zip
*.rar

# Docker specific
.dockerignore

# Kubernetes secrets
secrets/
*.keyring

# Local development overrides
local/
dev/

# Build artifacts that might be generated during testing
build/
dist/
target/

# Temporary script output
output/
results/

# Performance profiling
*.prof
*.pprof

# Heap dumps
*.hprof

# Core dumps
core
core.*

# Stack traces
*.stackdump

# Files with credentials
credentials.json
auth.json
.tokens
.secrets